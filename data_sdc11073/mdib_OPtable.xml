								<pm:ConceptDescription Lang="en-US">operation to set a specific slant-position</pm:ConceptDescription>
							</pm:Type>
						</pm:Operation>

						<pm:Operation
							xsi:type="pm:SetValueOperationDescriptor"
							Handle="m1.ch0.vmd1_sco"
