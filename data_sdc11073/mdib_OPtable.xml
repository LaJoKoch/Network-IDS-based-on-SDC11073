							MaxTimeToFinish="PT1S" AccessLevel="CSUsr">
							<pm:Type Code="134161"> <!-- MDC_ACT_PDMO_TXN 2::3089 -->
								<pm:ConceptDescription Lang="en-US">operation to configure the surface type</pm:ConceptDescription>
