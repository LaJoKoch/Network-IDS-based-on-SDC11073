								<pm:Type Code="192834">
									<pm:ConceptDescription Lang="en-US">ON</pm:ConceptDescription>
								</pm:Type>
							</pm:AllowedValue>
							<pm:AllowedValue>
								<pm:Value>OFF</pm:Value>
								<pm:Type Code="192835">
									<pm:ConceptDescription Lang="en-US">OFF</pm:ConceptDescription>
